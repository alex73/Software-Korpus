<!DOCTYPE html>
<html lang="en">
  <head>
    <meta charset="utf-8">
    <meta http-equiv="X-UA-Compatible" content="IE=edge">
    <meta name="viewport" content="width=device-width, initial-scale=1">

    <title>${title}</title>

    <!-- Bootstrap core CSS -->
    <link href="css/bootstrap.min.css" rel="stylesheet">
    <!-- IE10 viewport hack for Surface/desktop Windows 8 bug -->
    <link href="css/ie10-viewport-bug-workaround.css" rel="stylesheet">
    <!-- Custom styles for this template -->
    <link href="css/navbar-static-top.css" rel="stylesheet">
    <link rel="stylesheet" href="css/styles.css">
  </head>

  <body>

    <!-- Static navbar -->
    <nav class="navbar navbar-default navbar-fixed-top">
      <div class="container">
        <div class="navbar-header">
          <button type="button" class="navbar-toggle collapsed" onClick="c()" aria-expanded="false" aria-controls="navbar">
            <span class="sr-only">Toggle navigation</span>
            <span class="icon-bar"></span>
            <span class="icon-bar"></span>
            <span class="icon-bar"></span>
          </button>
          <a class="navbar-brand" href="#">Беларускі N-корпус</a>
        </div>
        <div id="navbar" class="navbar-collapse collapse">
          <ul class="nav navbar-nav navbar-right">
            <li <#if active == "index"   >class="active"</#if>><a href="index.html">Пра праект</a></li>
            <li <#if active == "korpus"  >class="active"</#if>><a href="korpus.html">Корпус</a></li>
            <li <#if active == "grammar" >class="active"</#if>><a href="grammar.html">Граматычная база</a></li>
            <li <#if active == "download">class="active"</#if>><a href="download.html">Спампаваць</a></li>
          </ul>
        </div>
      </div>
    </nav>
    <script>
      function c() {
        var el = document.getElementById('navbar');
        if(el.className.includes(' collapse')) {
          el.className = "navbar-collapse";
        } else {
          el.className = "navbar-collapse collapse";
        }
        return false;
      }
    </script>
